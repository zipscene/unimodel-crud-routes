{
  "name": "unimodel-crud-routes",
  "version": "0.1.13",
  "description": "CRUD route code for interfacing with unimodel",
  "main": "./dist/lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/zipscene/unimodel-crud-routes.git"
  },
  "keywords": [
    "unimodel",
    "crud"
  ],
  "scripts": {
    "lint": "node ./node_modules/.bin/eslint . || exit 0",
    "docs": "node ./node_modules/.bin/yuidoc .",
