  "description": "CRUD route code for interfacing with zs-unimodel",
  "main": "./dist/lib/index.js",
  "repository": {
    "type": "git",
    "url": "git@git.zipscene.com:zsapilibs/zs-unimodel-crud-routes.git"
  },
  "scripts": {
    "lint": "node ./node_modules/.bin/eslint . || exit 0",
